<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="TTnav.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:TTnav"
    xmlns:localViews="clr-namespace:TTnav.Views"
    Shell.FlyoutBehavior="Disabled"
    Title="TTnav">

    <ShellContent
        Title="Splash"
        ContentTemplate="{DataTemplate localViews:SplashPage}"
        Route="SplashPage"/>
    <ShellContent
        Title="Main"
        ContentTemplate="{DataTemplate localViews:MainPage}"
        Route="MainPage"
        />

</Shell>
